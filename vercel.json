{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "dist/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "dist/public/$1"
    }
  ]
}
{
  "version": 2,
  "rewrites": [
    { "source": "/api/(.*)", "destination": "dist/index.js" },
    { "source": "/(.*)", "destination": "dist/public/$1" }
  ]
}
